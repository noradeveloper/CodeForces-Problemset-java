import java.util.*;
public class TheatreSquare1A
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner input= new Scanner(System.in);
		int n, m, a;
	    n=input.nextInt();
		m=input.nextInt();
		a=input.nextInt();
		long flagsByRow;
		if (n % a == 0) 
			flagsByRow = n / a;
		
		else {
			flagsByRow = n / a;
            flagsByRow++;
	    }
	    
	    long flagsByColumn;
        if (m % a == 0)
            flagsByColumn = m / a;
        else {
            flagsByColumn = m / a;
            flagsByColumn++;
        }
        System.out.println(flagsByRow*flagsByColumn);
    
	}
}
